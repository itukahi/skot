<h1>ログイン</h1>

<div>
  <div>
    <%= form_for(:session, url: login_path) do |f| %>

        <%= f.label "アカウント" %>
        <%= f.text_field :account %>

        <%= f.label "パスワード" %>
        <%= f.password_field :password %>

        <%= f.submit "Log in" %>
    <% end %>

    <%= link_to "新規作成",signup_path %>
  </div>
</div>
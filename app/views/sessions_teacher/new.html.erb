<h1>SessionsTeacher#new</h1>
<p>Find me in app/views/sessions_teacher/new.html.erb</p>


<h1>Log in</h1>

<% if flash[:notice] %>
  <p><%= flash[:notice] %><p>
<% end %>

<% if flash[:danger] %>
  <p><%= flash[:danger] %><p>
<% end %>

<div>
  <div>
    <%= form_for(:session_teacher, url: login_teacher_path) do |f| %>

        <%= f.label "アカウント" %>
        <%= f.text_field :account %>

        <%= f.label "パスワード" %>
        <%= f.password_field :password %>

        <%= f.submit "ログイン" %>
    <% end %>

  </div>
</div>